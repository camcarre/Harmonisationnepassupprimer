<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>da54b865-29d6-4398-9512-c69513b46740</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Harmonisation Add-in</ProviderName>
  <DefaultLocale>fr-FR</DefaultLocale>
  <DisplayName DefaultValue="Harmonisation Add-in"/>
  <Description DefaultValue="Add-in pour l'harmonisation des données Excel."/>
  <IconUrl DefaultValue="https://localhost:5890/assets/icon-64.png"/>
  <HighResolutionIconUrl DefaultValue="https://localhost:5890/assets/icon-80.png"/>
  <SupportUrl DefaultValue="https://localhost:5890"/>
  <AppDomains>
    <AppDomain>https://localhost:5890</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://localhost:5890/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://localhost:5890/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://localhost:5890/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://localhost:5890/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://localhost:5890"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://localhost:5890/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Harmonisation Add-in"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="Harmonisation"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Ouvrir"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="L'add-in d'harmonisation est prêt à être utilisé. Cliquez sur le bouton 'Ouvrir' dans l'onglet Accueil pour commencer."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Cliquez pour ouvrir l'add-in d'harmonisation"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>